{
  "expo": {
    "name": "inFinance",
    "slug": "infinance",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "infinance",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#22c55e"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icons/ios/AppIcon~ios-marketing.png",
      "bundleIdentifier": "com.infinance.app"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.infinance.app",
      "icon": "./assets/icons/android/play_store_512.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/android/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    }
  }
}
